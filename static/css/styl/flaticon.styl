@font-face
	font-family: 'Flaticon'
	src: url($font_path'flaticon.eot')
	src: url($font_path'flaticon.eot#iefix') format('embedded-opentype'),
		url($font_path'flaticon.woff') format('woff'),
		url($font_path'flaticon.ttf') format('truetype'),
		url($font_path'flaticon.svg') format('svg')
	font-weight: normal
	font-style: normal

[class^='flaticon-']:before, [class*=' flaticon-']:before,
[class^='flaticon-']:after, [class*=' flaticon-']:after
	font-family: Flaticon
	font-style: normal

.flaticon-
	&check59:before 
		content: '\e000'
	&email6:before 
		content: '\e001'
	&facebook29:before
		content: '\e002'
	&github13:before 
		content: '\e003'
	&hotel75:before 
		content: '\e004'
	&phone72:before 
		content: '\e005'
	&photos12:before 
		content: '\e006'
	&social19:before 
		content: '\e007'
	&social5:before 
		content: '\e008'
	&social6:before 
